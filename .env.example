# Base RPC URL for forking mainnet
BASE_RPC_URL=https://mainnet.base.org

# Enable forking for production tests
FORK_ENABLED=false

# Enable real transactions in production tests (use with caution)
RUN_REAL_TXS=false

# Enable gas reporting
REPORT_GAS=false

# Private key for deployment (keep this secret!)
# PRIVATE_KEY=your_private_key_here
